<!-- @format -->

<div class="container mt-5">
  <div class="card">
    <div class="card-header">
      <a class="btn btn-primary" href="/pharmacy/customers/create">create</a>
    </div>
    <div class="card-body text-center">
      <h1>Items</h1>
      <table class="table table-dark table-striped">
        <thead>
          <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Price</th>
            <th scope="col">Quntitiy</th>
            <th scope="col">catagory_id</th>
          </tr>
        </thead>
        <tbody>
          <% items.forEach(function(items){ %>
          <tr>
            <th scope="row"><%= items.id %></th>
            <th scope="row"><%= items.item_name %></th>
            <th scope="row"><%= items.item_price %></th>
            <th scope="row"><%= items.item_quntitny %></th>
            <th scope="row"><%= items.catagory_id %></th>
            <td>
              <div></div>
            </td>
          </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
  </div>
</div>
